{
  "expo": {
    "name": "Fake store",
    "slug": "fake-store",
    "version": "1.0.0",
    "sdkVersion": "44.0.0",
    "scheme": "fake-store",
    "icon": "./assets/icon.png",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.joseito.fakestore",
      "associatedDomains": ["applinks:https://universal-fake-store.vercel.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.joseito.terence.fakestore",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "universal-fake-store.vercel.app",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
